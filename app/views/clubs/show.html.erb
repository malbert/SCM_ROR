<p id="notice"><%= notice %></p>


<%= link_to 'Edit', edit_club_path(@club) %> |
<%= link_to 'Back', clubs_path %>



<div id="content">
  <div id="members">
    <h3><%=t('club.members')%></h3>
    
    <table class="table">
    <tr>
      <th><%=t('club.member_table.name')%></th>
      <th><%=t('club.member_table.email')%></th>
      <th><%=t('club.member_table.phone')%></th>
      <th><%=t('club.member_table.isOk')%></th>
    </tr>

    <% @club.members.each do |member| %>
      <tr id="member_<%=member.id%>">
      <td><%= link_to member.firstname + ' ' + member.lastname, member%> </td>
        <td><%=member.email%></td>
        <td><%=member.phone%></td>
        <td><%= link_to t('dashboard.club_table.club_edit'), edit_club_path(club) %></td>
      </tr>
    <% end %>
  </table>
    
    
  </div>
  
</div>
